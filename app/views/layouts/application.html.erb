<!DOCTYPE html>
<html>
  <head>
    <title>TravelBot</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>


  <nav>
  <div class="nav-wrapper">
    <a href="#" class="brand-logo">TravelBot</a>
    <ul id="nav-mobile" class="right hide-on-med-and-down">

      <li><% if session[:user_id] %></li>
      <li>  <%= link_to "Home", user_path(session[:user_id]) %></li>
      <li>  <%= link_to "logout", session_path(session[:user_id]), :method=>:delete %></li>
        <% end %>
          <li><%= link_to "Travel Inspiration"%></li>
    </ul>
  </div>
</nav>


  <body>
    <div class="container">
      <%= yield %>


    </div>


  </body>
</html>
